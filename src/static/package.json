{
  "name": "pgblackboard-ui",
  "description": "pgBlackboard ui",
  "devDependencies": {
    "autoprefixer-core": "~5.1.7",
    "closurecompiler": "^1.5.1",
    "csso": "~1.3.11",
    "jake": "~8.0.12",
    "jasmine": "~2.2.1",
    "jscs": "~1.11.3",
    "less": "~2.4.0",
    "require-list": "~0.1.6",
    "uglify-js": "~2.4.17"
  },
  "dependencies": {
    "codemirror": "~5.0.0",
    "knockout": "~3.3.0",
    "leaflet": "~0.7.3"
  },
  "version": "0.0.0",
  "scripts": {
    "test": "jasmine",
    "build": "jake",
    "jshint": "jshint src --exclude src/bundle-index.js",
    "jscs": "jscs src --exclude src/bundle-index.js --verbose"
  },
  "author": "",
  "license": "BSD-2-Clause",
  "jshintConfig": {
    "sub": true
  },
  "jscsConfig": {
    "preset": "yandex",
    "esnext": true,
    "requireDotNotation": null,
    "disallowSpacesInsideObjectBrackets": null,
    "disallowQuotedKeysInObjects": null,
    "disallowMultipleVarDecl": "exceptUndefined",
    "validateIndentation": 4
  }
}
